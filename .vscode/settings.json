{
    "files.autoSave": "afterDelay",
    "explorer.confirmDelete": false,
    "editor.fontSize": 14,
    "editor.codeActionsOnSave": null,
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=c++17 $fileName -o $workspaceRoot/.vscode/output/$fileNameWithoutExt && $workspaceRoot/.vscode/output/$fileNameWithoutExt",
    },
    "files.associations": {
        "chrono": "cpp",
        "vector": "cpp",
        "ostream": "cpp",
        "xstring": "cpp",
        "iostream": "cpp",
        "xutility": "cpp",
        "unordered_map": "cpp",
        "__hash_table": "cpp",
        "functional": "cpp",
        "string": "cpp",
        "iosfwd": "cpp",
        "__tree": "cpp",
        "map": "cpp",
        "memory": "cpp"
    }
}